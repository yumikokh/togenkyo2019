{
  "name": "vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.21",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "postcss-assets": "^5.0.0",
    "read-config": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.21",
    "yaml-loader": "^0.5.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "vue-cli-service lint",
        "git add"
      ]
    },
    "ignore": [
      "dist/**/*.js"
    ]
  }
}
